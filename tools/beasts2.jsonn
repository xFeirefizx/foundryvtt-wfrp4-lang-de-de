          "img": "systems/wfrp4e/icons/traits/trait.png",
          "id": "Frenzy",
          "_id": "onATYjyFvqAOIFDt",
          "name": "Frénésie",
          "data": {
            "rollable": {
              "value": true,
              "bonusCharacteristic": "",
              "defaultDifficulty": "challenging",
              "rollCharacteristic": "wp",
              "label": "Rollable",
