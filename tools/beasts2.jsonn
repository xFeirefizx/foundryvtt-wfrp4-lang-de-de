          },
          "img": "systems/wfrp4e/icons/traits/trait.png",
          "id": "Corruption",
          "_id": "DpYOWHWmBKACi60D",
          "name": "Corruption",
          "data": {
            "rollable": {
              "value": false,
              "bonusCharacteristic": "",
              "rollCharacteristic": "",
              "label": "Rollable",
              "type": "Boolean"
            },
            "specification": {
              "value": "Minor",
              "label": "Specification",
              "type": "String"
            },
            "description": {
              "label": "Description",
              "value": "The creature is tainted by Chaos, or perhaps suﬀused with Dark Magics. The Strength of the Corruption is marked in brackets.",
              "type": "String"
            },
            "source": {
              "label": "Source",
