            }
          },
          "type": "trait"
        },
        {
          "flags": {
            "_sheetTab": "details"
          },
          "img": "systems/wfrp4e/icons/traits/trait.png",
          "id": "Size",
          "_id": "kTCUCyy5eRbdAUrO",
          "name": "Taille",
          "data": {
            "rollable": {
              "value": false,
              "bonusCharacteristic": "",
              "defaultDifficulty": "challenging",
              "rollCharacteristic": "",
              "label": "Rollable",
              "type": "Boolean"
            },
            "specification": {
              "value": "Grande",
